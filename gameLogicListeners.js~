$(document).ready(function(){
    
    function addLabels(){
        $('div#meta > span').each(function(){
            //var id = $(this).id();
            $(this).text($(this).attr('id') + ': ');
        });
    }
    

    
    addLabels();
});

$(window).keypress(function(event){
    console.log(event.keycode);
});
